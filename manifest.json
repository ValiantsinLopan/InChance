{
    "manifest_version": 2,
    "name": "Insta Raffle",
    "description": "Play prizes using comments in Instagram",
    "version": "0.1",
    "permissions": ["tabs", "declarativeContent"], 
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action":{
        "default_icon": "/images/128.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "js": ["jquery-2.1.3.min.js", "content.js"]
        }
      ]
    }